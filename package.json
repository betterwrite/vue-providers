{
  "name": "vue-directive-providers",
  "author": "Novout",
  "license": "MIT",
  "version": "0.1.0",
  "description": "A simple collection of general-purpose directives for Vue 2/3.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/betterwrite/vue-directive-providers"
  },
  "scripts": {
