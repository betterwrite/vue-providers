  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.6",
    "@types/node": "^14.14.22",
    "@vitejs/plugin-vue": "^3.1.2",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.1.3",
    "vite": "^3.1.4",
    "vue": "^3.0.0"
  },
  "files": [
    "dist",
    "package.json"
